<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <com.app.rideWhiz.widget.TextViewRegular
        android:id="@+id/txt_choose_group"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="@string/choose_group_text"
        android:padding="@dimen/_7sdp"
        android:textAlignment="textStart"
        android:textColor="@color/loginbtn"
        android:textSize="@dimen/_14sdp"
        tools:ignore="RtlCompat" />


    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/_1sdp"
        android:layout_below="@+id/txt_choose_group"
        android:background="@color/loginbtn" />
</RelativeLayout>
